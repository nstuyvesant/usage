            <xml-property name="queryText"><![CDATA[SELECT usage_type,initiation_time AT TIME ZONE 'UTC' AS initiation_time,start_time AT TIME ZONE 'UTC' AS start_time,end_time AT TIME ZONE 'UTC' AS end_time,tokens_charged,tokens_used,username,initiated_by,manufacturer,model,first_name,last_name,company_name,email,
				user_groups,user_roles, ';' || string_agg(dev_roles.dev_role,';' ORDER BY dev_roles.dev_role COLLATE "C") || ';' AS device_roles,distributor,all_w_roles.handset_id,charging_or_reservation_id,modified
				,os, os_version, location
	FROM 
		(
		--below select: all_w_roles - contains all info with user groups
		SELECT usage_type,start_time,end_time,tokens_charged,tokens_used,username,initiated_by,all_w_groups.handset_id,manufacturer,model,distributor,first_name,last_name,company_name
						,charging_or_reservation_id,initiation_time,email, all_w_groups.uid AS uid,all_w_groups.hid AS hid,user_groups,';' || string_agg(roles.role,';' ORDER BY roles.role COLLATE "C") || ';' AS user_roles,modified
						,os, os_version, location
			FROM 
				(
				--below select: all_w_groups - contains all info with user groups
				SELECT usage_type,start_time,end_time,tokens_charged,tokens_used,username,initiated_by,
						all_with_ids.handset_id,manufacturer,model,distributor,first_name,last_name,company_name
						,charging_or_reservation_id,initiation_time,email, all_with_ids.uid AS uid,all_with_ids.hid AS hid,string_agg(groups.label,';') AS user_groups,modified
						,os, os_version, location
					FROM
						--below select: all_with_ids - contains all info with uid and hid but w/o the roles and groups 
						(
						SELECT usage_type, start_time,end_time,tokens_charged,tokens_used,username,initiated_by,handset_id,manufacturer,model,distributor,
									first_name,last_name,company_name,charging_or_reservation_id,initiation_time,np_user1.email, np_user1.id AS uid,np_handset1.id AS hid,modified
									,os, os_version, np_handset1.location AS location
							FROM
								-- below select: all_without_user_and_handset_info : all info without the user and handset information	
								(
								SELECT 	'Session' AS usage_type,	 
										MIN(start_time) AS start_time,MAX(end_time) AS end_time,SUM(num_of_tokens) AS tokens_charged
										,SUM(num_of_tokens):: numeric AS tokens_used,
										np_audit_billing_usage_event.username AS username,np_audit_billing_usage_event.username AS initiated_by,handsets AS handset_id,charging_id AS charging_or_reservation_id,
										MIN(start_time) AS initiation_time,null AS modified
									FROM np_audit_billing_usage_event
									JOIN np_handset
										ON np_audit_billing_usage_event.handsets = np_handset.name 
									JOIN np_user
										ON np_audit_billing_usage_event.username = np_user.name 
									WHERE 
										billing_trigger_type in ('INTERACTIVE', 'ALLOCATION_CONTINUE', 'SCRIPT')
										-- the next AND line should be commented out before checking in
										-- AND end_time AT TIME ZONE 'UTC' >= '2013-01-01' AND end_time AT TIME ZONE 'UTC' < '2013-01-22'							
										-- the next 2 lines should be un-commented before checking in. 
										AND end_time AT TIME ZONE 'UTC' >= ? AND end_time AT TIME ZONE 'UTC' < ? 
										AND np_user.email LIKE ? 
										AND LOWER(np_user.email) NOT LIKE '%@perfectomobile.com' 		   
										AND np_audit_billing_usage_event.status = 'Success'
									GROUP BY np_audit_billing_usage_event.username, handsets,charging_id								
									
								UNION

								-- this is the body of all_without_user_and_handset_info
								SELECT 'Reservation' AS usage_type,
										start_time,end_time,num_of_tokens AS tokens_charged,
										
										CEIL( (COALESCE(durations,0) 				-- subtract the amount to be subtracted
											- GREATEST(0,COALESCE(closed_duration1.duration_in_millis,0) 					--- the total time of the first handset session that closed inside the reservation
													- COALESCE(EXTRACT(EPOCH FROM (first_close - start_time)) * 1000. , 0)) 	--- subtract the time that was used before the reservation started
																					-- add the amount to be added 
											+ GREATEST(0,COALESCE(closed_duration2.duration_in_millis,0) 				--- the total time of the first handset session that closed after the reservation
													- COALESCE(EXTRACT(EPOCH FROM (close_after - end_time)) * 1000. , 0)    --- subtract the time that was used after the reservation ended
													- GREATEST(0,COALESCE(closed_duration2.duration_in_millis,0) 
															- COALESCE(EXTRACT(EPOCH FROM (close_after - start_time)) * 1000. , 0)))  --- subtract the time that was used before the reservation started				
											) / 1000. / 60.) :: numeric AS tokens_used,
										
										usr AS username,initiated_by,handsets AS handset_id,
										orig_with_all_close_durations.reservation_id AS charging_or_reservation_id, reservation_for_creation.event_time AS initiation_time,reservation_updates_or_deletions.reservation_id AS modified
										
										--,durations, sessions, first_close, closed_duration1.duration_in_millis AS first_close_duration, close_after, closed_duration2.duration_in_millis AS close_after_duration,  
										---GREATEST(0,COALESCE(closed_duration1.duration_in_millis,0) 				--- the total time of the first handset session that closed inside the reservation
										---		- COALESCE(EXTRACT(EPOCH FROM (first_close - start_time)) * 1000. , 0) 	--- subtract the time that was used before the reservation started
										---	 )  AS subtract,
										---GREATEST(0,COALESCE(closed_duration2.duration_in_millis,0) 				--- the total time of the first handset session that closed after the reservation
										--- 		- COALESCE(EXTRACT(EPOCH FROM (close_after - end_time)) * 1000. , 0)    --- subtract the time that was used after the reservation ended
										---		- GREATEST(0,COALESCE(closed_duration2.duration_in_millis,0) - COALESCE(EXTRACT(EPOCH FROM (close_after - start_time)) * 1000. , 0))  --- subtract the time that was used before the reservation started
										---	 )  AS addd,
										---COALESCE(durations,0) 					-- subtract the amount to be subtracted
										---	- GREATEST(0,COALESCE(closed_duration1.duration_in_millis,0) 					--- the total time of the first handset session that closed inside the reservation
										---			- COALESCE(EXTRACT(EPOCH FROM (first_close - start_time)) * 1000. , 0)) 	--- subtract the time that was used before the reservation started
										---							-- add the amount to be added 
										---	+ GREATEST(0,COALESCE(closed_duration2.duration_in_millis,0) 				--- the total time of the first handset session that closed after the reservation
										---			- COALESCE(EXTRACT(EPOCH FROM (close_after - end_time)) * 1000. , 0)    --- subtract the time that was used after the reservation ended
										---			- GREATEST(0,COALESCE(closed_duration2.duration_in_millis,0) 
										---					- COALESCE(EXTRACT(EPOCH FROM (close_after - start_time)) * 1000. , 0)))  --- subtract the time that was used before the reservation started
										---  AS total_usage_milis,

											
									FROM
										(		
										-- this is the body of orig_with_all_close_durations	
										SELECT start_time,end_time,handsets,usr,initiated_by,num_of_tokens,durations, sessions, first_close, MIN(close_after) AS close_after, reservation_id
											FROM
												(		
												-- this is the body of orig_with_all close_events
												SELECT start_time,end_time,handsets,usr,initiated_by,num_of_tokens,durations, sessions, first_close, closed_after.event_time AS close_after, reservation_id
													FROM
														(		
														-- this is the body of orig_with_first_close						
														SELECT start_time,end_time,handsets,usr,initiated_by,CEIL(reservation_duration / 60. / 1000.) AS num_of_tokens,SUM(duration_in_millis) AS durations, count(duration_in_millis) AS sessions, MIN(close_time) AS first_close,reservation_id
															FROM
																(		
																-- this is the body of orig_with_close_times						
																SELECT start_time,end_time,handsets,usr,initiated_by,reservation_duration, closed.event_time AS close_time, closed.duration_in_millis, reservation_id
																	FROM																		
																		(SELECT start_time,end_time,handset_id AS handsets,reserved_to AS usr, username AS initiated_by,
																				--- duration_in_millis AS reservation_duration, 
																				--- Note: due to code bug, this is more than the actual reservation. revert to above line after it's fixed
																				--- Note2: potentially could use np_audit_billing_usage_event but it requires substential understanding of that table
																				EXTRACT(EPOCH FROM (end_time - start_time)) * 1000. AS reservation_duration,
																				np_audit_reservation_event.reservation_id
																			FROM 
																				--below select: get the last rec of each reservation 
																				(SELECT reservation_id, MAX(id) AS id
																					FROM np_audit_reservation_event 
																					--ignore records that indicate failure, AS well AS records that indicate there was an overdraft (in which case 2 records have been written)
																					WHERE status = 'Success'
																						AND reservation_id IS NOT NULL
																						-- the next AND line should be commented out before checking in and the less than needs to be changed to ampersand lt semicolon
																						-- AND end_time AT TIME ZONE 'UTC' >= '2013-01-01' AND end_time AT TIME ZONE 'UTC' < '2013-01-22'
																						-- the next line should be un-commented before checking in. 
																						AND end_time AT TIME ZONE 'UTC' >= ? AND end_time AT TIME ZONE 'UTC' < ?
																					GROUP BY reservation_id
																				) AS finished_reservations
																			JOIN	np_audit_reservation_event
																				ON finished_reservations.id = np_audit_reservation_event.id
																				--np_user is brought in only to make sure that the user's email is not @perfectomobile.com
																			JOIN	np_user
																				ON np_audit_reservation_event.reserved_to = np_user.name
																			JOIN	np_account --- needed to eliminate unlimited-tokens users (see also last where clause)
																				ON np_user.account_id = np_account.id 
																			WHERE np_account.unit_cost >= 1																				
																				-- the next line should be un-commented before checking in.
																				AND np_user.email LIKE ? 
																				AND LOWER(np_user.email) NOT LIKE '%@perfectomobile.com'
																		) AS orig
																		--now join with all the handset close events
																		LEFT OUTER JOIN
																			(SELECT * FROM np_audit_handset_event WHERE event_type = 'HS_CLOSE' AND status = 'Success') AS closed
																		ON closed.event_time >= orig.start_time AND closed.event_time < orig.end_time 
																			AND orig.handsets = closed.handset_id AND usr = closed.username
																) AS orig_with_close_times
															GROUP BY start_time,end_time,handsets,usr,initiated_by,reservation_duration, reservation_id
															
														) AS orig_with_first_close		
														--now join with all the handset close events after the reservation
														LEFT OUTER JOIN
															(SELECT * FROM np_audit_handset_event WHERE event_type = 'HS_CLOSE' AND status = 'Success') AS closed_after
														ON closed_after.event_time >= orig_with_first_close.end_time  
															AND orig_with_first_close.handsets = closed_after.handset_id AND usr = closed_after.username
												) AS orig_with_all_close_events
											GROUP BY start_time,end_time,handsets,usr,initiated_by,num_of_tokens,reservation_id,durations, sessions, first_close
										) AS orig_with_all_close_durations
										--now join with the duration of the first close within the reservation and the first close after the reservation
										LEFT OUTER JOIN
											(SELECT * FROM np_audit_handset_event WHERE event_type = 'HS_CLOSE' AND status = 'Success') AS closed_duration1
										ON closed_duration1.event_time = orig_with_all_close_durations.first_close  
											AND orig_with_all_close_durations.handsets = closed_duration1.handset_id AND usr = closed_duration1.username
										--now join with the duration of the first close after the reservation and the first close after the reservation
										LEFT OUTER JOIN
											(SELECT * FROM np_audit_handset_event WHERE event_type = 'HS_CLOSE' AND status = 'Success') AS closed_duration2
										ON closed_duration2.event_time = orig_with_all_close_durations.close_after  
											AND orig_with_all_close_durations.handsets = closed_duration2.handset_id AND usr = closed_duration2.username
										--join this to attach the reservation creation time
										LEFT OUTER JOIN 
											(SELECT event_time,event_type,reservation_id,status FROM np_audit_reservation_event) AS reservation_for_creation
										ON reservation_for_creation.status = 'Success' AND reservation_for_creation.event_type = 'RESERVATION_ADD'
										 AND orig_with_all_close_durations.reservation_id = reservation_for_creation.reservation_id
										--join this to attach reservation that were updated or deleted  
										LEFT OUTER JOIN 
											(SELECT reservation_id 
											 FROM np_audit_reservation_event 
											 WHERE status='Success' AND (event_type = 'RESERVATION_DELETE' OR event_type = 'RESERVATION_UPDATE')
											 GROUP BY reservation_id 
											 ) AS reservation_updates_or_deletions
										ON reservation_updates_or_deletions.reservation_id = orig_with_all_close_durations.reservation_id
								) AS all_without_user_and_handset_info
								--join this to attach email and user id
								LEFT OUTER JOIN 
									np_user AS np_user1
								ON all_without_user_and_handset_info.username = np_user1.name 
								--join this to attach handset id
								LEFT OUTER JOIN 
									np_handset AS np_handset1
								ON all_without_user_and_handset_info.handset_id = np_handset1.name
						) AS all_with_ids
						-- now join np_user_group to this
						LEFT OUTER JOIN
							(SELECT user_id,group_id FROM np_user_group) AS usergroups
						ON all_with_ids.uid = usergroups.user_id
						-- now join np_group to the np_user_group
						LEFT OUTER JOIN
							(SELECT id,label FROM np_group) AS groups
						ON groups.id = usergroups.group_id	 
					GROUP BY usage_type,start_time,end_time,tokens_charged,tokens_used,username,initiated_by,handset_id,manufacturer,model,distributor,
						first_name,last_name,company_name,charging_or_reservation_id,initiation_time,email, uid,hid,modified
						,os, os_version, location
				) AS all_w_groups
				-- now join np_user_role to this
				LEFT OUTER JOIN
					(SELECT user_id,role_id FROM np_user_role) AS userroles 
				ON all_w_groups.uid = userroles.user_id
				-- now join np_role the np_user_role
				LEFT OUTER JOIN
					(SELECT id,name AS role FROM np_role) AS roles
				ON roles.id = userroles.role_id
			GROUP BY usage_type,start_time,end_time,tokens_charged,tokens_used,username,initiated_by,handset_id,manufacturer,model,distributor,
					first_name,last_name,company_name,charging_or_reservation_id,initiation_time, email, uid,hid,user_groups,modified
					,os, os_version, location
		) AS all_w_roles
		-- now we have the original query + email + user groups + user roles
		-- now join np_handset_role to this
		LEFT OUTER JOIN
			(SELECT handset_id AS hid1,role_id FROM np_handset_role) AS deviceroles 
		ON all_w_roles.hid = deviceroles.hid1
		-- now join np_role to the np_handset_role
		LEFT OUTER JOIN
			(SELECT id,name AS dev_role FROM np_role) AS dev_roles
		ON dev_roles.id = deviceroles.role_id
	GROUP BY usage_type,start_time,end_time,tokens_charged,tokens_used,username,initiated_by,handset_id,manufacturer,model,distributor,
			first_name,last_name,company_name,charging_or_reservation_id,initiation_time,email,user_groups, user_roles,modified
			,os, os_version, location
UNION
SELECT 'Credit' AS usage_type,event_time AT TIME ZONE 'UTC' AS initiation_time,event_time AT TIME ZONE 'UTC' AS start_time,event_time AT TIME ZONE 'UTC' AS end_time,
		0-num_of_tokens AS tokens_charged,0 AS tokens_used,username,null AS initiated_by,null AS manufacturer,null AS model,
		first_name,last_name,company_name,email,null AS user_groups,null AS user_roles, null AS device_roles,null AS distributor,null AS handset_id,token_order_id AS charging_or_reservation_id,null AS modified
		,null AS os, null AS os_version, null AS location
	FROM
		(SELECT * 
			FROM np_audit_billing_admin_event 
			WHERE event_type = 'ORDER_CREATE' AND status = 'Success'
				-- the next AND line should be commented out before checking in
				-- AND event_time AT TIME ZONE 'UTC' >= '2013-01-01' AND event_time AT TIME ZONE 'UTC' < '2013-01-22'
				-- the next line should be un-commented before checking in. 
				AND event_time AT TIME ZONE 'UTC' >= ? AND event_time AT TIME ZONE 'UTC' < ? 
		) AS credits
		LEFT OUTER JOIN 
			np_user AS credits_users
		ON credits.username = credits_users.name
	WHERE
		LOWER(credits_users.email) NOT LIKE '%@perfectomobile.com'
		-- the next line should be un-commented before checking in.
		AND credits_users.email LIKE ?
ORDER BY start_time DESC, handset_id
LIMIT 10000]]></xml-property>
